/* WEBKIT */
@-webkit-keyframes getDrawerIn {
    from { -webkit-transform: translate3d(-80%, 0, 0); }
    to   { -webkit-transform: translate3d(0, 0, 0); }
}
@-webkit-keyframes getDrawerOut {
    // from { -webkit-transform: translate3d(0,0,0); }
    // to   { -webkit-transform: translate3d(80%,0,0); } // slides along like

    // from { -webkit-transform: scale3d(1,1,1) }
    // to   { -webkit-transform: scale3d(0.9, 0.9, 0.9) } // fade out like

    from { -webkit-transform: translate3d(0,0,0);  } //opacity: 1.0;
    to   { -webkit-transform: translate3d(0,0,0);  } // nothin'opacity: 0.5;
}


@-webkit-keyframes delDrawerIn {
    // to   { -webkit-transform: translate3d(80%,0,0) !important; }
    // from { -webkit-transform: translate3d(0,0,0) !important; } // slides along like

    // from { -webkit-transform: translate3d(0,0,0) scale3d(0.9, 0.9, 0.9); }
    // to   { -webkit-transform: translate3d(0,0,0) scale3d(1,1,1); } // fade out like

    from { -webkit-transform: translate3d(0,0,0); } // opacity: 0.5;
    to   { -webkit-transform: translate3d(0,0,0); } // nothin'opacity: 1.0;
}
@-webkit-keyframes delDrawerOut {
    from   { -webkit-transform: translate3d(0, 0, 0) }
    to { -webkit-transform: translate3d(-100%, 0, 0) }
}




@-webkit-keyframes moveLeftIn {
    // from { -webkit-transform: translate3d(100%,0,0); }
    // to   { -webkit-transform: translate3d(0,0,0); }
    from { -webkit-transform: translate3d(100%,0,0); z-index: 99999; }
    to   { -webkit-transform: translate3d(0,0,0); z-index: 99999; }
}
@-webkit-keyframes moveLeftOut {
    // from { -webkit-transform: translate3d(0, 0, 0) scale3d(1, 1, 1); }
    // to   { -webkit-transform: translate3d(-50%, 0, 0) scale3d(0.90, 0.90, 0.90); }
    // from { -webkit-transform: translate3d(0, 0, 0); opacity: 1; }
    // to   { -webkit-transform: translate3d(-50%, 0, 0); opacity: 0; }
    from { -webkit-transform: translate3d(0, 0, 0); }
    to   { -webkit-transform: translate3d(-50%, 0, 0); }
}

/* moveRight */
@-webkit-keyframes moveRightIn {
    from { -webkit-transform: translate3d(-100%, 0, 0); }
    to   { -webkit-transform: translate3d(0, 0, 0); }
}
@-webkit-keyframes moveRightOut {
    from { -webkit-transform: translate3d(0,0,0); }
    to   { -webkit-transform: translate3d(100%,0,0); }
}

// Push
@-webkit-keyframes pushUpIn {
    from { -webkit-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, 150%, 0); z-index: 9; }
    to   { -webkit-transform: scale3d(1   , 1   , 1   ) translate3d(0,    0, 0); z-index: 9; }
}

@-webkit-keyframes pushUpOut {
    from { -webkit-transform: scale3d(1   , 1,    1   ) translate3d(0,    0, 0); z-index: 8; }
    to   { -webkit-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, -1em, 0); z-index: 8; }
}

// Push
@-webkit-keyframes pushDownIn {
    from { -webkit-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, -1em, 0); z-index: 8; } // opacity: 0.7;
    to   { -webkit-transform: scale3d(1   , 1,    1   ) translate3d(0,    0, 0); z-index: 8; } // opacity: 1.0;
}

@-webkit-keyframes pushDownOut {
    from { -webkit-transform: translate3d(0,    0, 0); z-index: 9; }
    to   { -webkit-transform: translate3d(0, 110%, 0); z-index: 9; }
    
}

