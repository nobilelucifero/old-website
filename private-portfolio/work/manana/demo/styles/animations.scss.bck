// webkit
@-webkit-keyframes rotate { 
    from { -webkit-transform: rotate(0); }
    to   { -webkit-transform: rotate(360deg);    }
}



/* MOZ */
@-moz-keyframes getDrawerIn {
    from { -moz-transform: translate3d(-80%, 0, 0); }
    to   { -moz-transform: translate3d(0, 0, 0); }
}
@-moz-keyframes getDrawerOut {
    from { -moz-transform: translate3d(0,0,0); }
    to   { -moz-transform: translate3d(0,0,0); }
}
@-moz-keyframes delDrawerIn {
    from { -moz-transform: translate3d(0,0,0); }
    to   { -moz-transform: translate3d(0,0,0); } // nothin'
}
@-moz-keyframes delDrawerOut {
    from   { -moz-transform: translate3d(0, 0, 0); }
    to { -moz-transform: translate3d(-100%, 0, 0); }
}



// MOZ
@-moz-keyframes moveLeftIn {
    from { -moz-transform: translate3d(100%,0,0); z-index: 9; }
    to   { -moz-transform: translate3d(0,0,0); z-index: 9; }
}
@-moz-keyframes moveLeftOut {
    from { -moz-transform: translate3d(0, 0, 0); z-index: 8; }
    to   { -moz-transform: translate3d(-50%, 0, 0); z-index: 8; }
}

@-moz-keyframes moveRightIn {
    from { -moz-transform: translate3d(-100%, 0, 0); z-index: 9; }
    to   { -moz-transform: translate3d(0, 0, 0); z-index: 9; }
}
@-moz-keyframes moveRightOut {
    from { -moz-transform: translate3d(0,0,0); z-index: 8; }
    to   { -moz-transform: translate3d(100%,0,0); z-index: 8; }
}

@-moz-keyframes pushUpIn {
    from { -moz-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, 150%, 0); z-index: 9; }
    to   { -moz-transform: scale3d(1   , 1   , 1   ) translate3d(0,    0, 0); z-index: 9; }
}

@-moz-keyframes pushUpOut {
    from { -moz-transform: scale3d(1   , 1,    1   ) translate3d(0,    0, 0); z-index: 8; }
    to   { -moz-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, -1em, 0); z-index: 8; }
}

@-moz-keyframes pushDownIn {
    from { -moz-transform: scale3d(0.90, 0.90, 0.90) translate3d(0, -1em, 0); z-index: 8; } // opacity: 0.7;
    to   { -moz-transform: scale3d(1   , 1,    1   ) translate3d(0,    0, 0); z-index: 8; } // opacity: 1.0;
}

@-moz-keyframes pushDownOut {
    from { -moz-transform: translate3d(0,    0, 0); z-index: 9; }
    to   { -moz-transform: translate3d(0, 110%, 0); z-index: 9; }
    
}





